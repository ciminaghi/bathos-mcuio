#!/bin/sh
#set -x

[ $# -gt 1 ] && out=$1 || out=/dev/stdout

echo "# Automatically generated by $0 on $(shell date)" > $out

for v in CFLAGS LDFLAGS CROSS_COMPILE AS LD CC CPP AR NM STRIP OBJCOPY OBJDUMP \
    BATHOS_LIBS BATHOS_FINAL_RELOCATABLE_LDFLAGS ; do
    eval [ -n \"\${${v}}\" ] || { echo "$0: error $v is empty" ; exit 127 ; }
    eval echo $v = \$${v} >> $out
done
